ls = import["../../../dev_env/imports/list.gl"]

record_to__list = [record] => {
	_list = []

	for [record._values.[0]] => {
	    _list = _list + @{}
    }

	for [_list index] => {
	    for [record._keys i k] => {
		    _list.[index] = _list.[index] + @{ record._keys.[i]: record._values.[i].[index] }
	    }
    }

	ret _list
}

select = [func _list] => {
	ret _list -> ls.filter[func]
}  