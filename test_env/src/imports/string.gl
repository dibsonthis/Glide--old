split = [delim x] => {
	if [delim == ""] => {
		ret x -> chars
	}
	
    new = []
    _list = x -> chars
    s = ""

    for[_list i c] => {
        if => {
            c == delim: {
                new = new + s
                s = ""
            }
            default: {
                s = s + c
            }
        }
    }

    new = new + s

    ret new
}
