ls = import["src/imports/list.gl"]

chars = [string] => {
	ret string.to_chars
}

upper = [string] => {
	if [string == ""] => {
		ret ""
	}

	_list = string -> chars
	for [_list i c] => {
		if => {
			c == "a": { _list.[i] = "A" }
			c == "b": { _list.[i] = "B" }
			c == "c": { _list.[i] = "C" }
			c == "d": { _list.[i] = "D" }
			c == "e": { _list.[i] = "E" }
			c == "f": { _list.[i] = "F" }
