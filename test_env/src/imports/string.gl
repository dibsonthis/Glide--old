			c == "i": { _list.[i] = "I" }
			c == "j": { _list.[i] = "J" }
			c == "k": { _list.[i] = "K" }
			c == "l": { _list.[i] = "L" }
			c == "m": { _list.[i] = "M" }
			c == "n": { _list.[i] = "N" }
			c == "o": { _list.[i] = "O" }
			c == "p": { _list.[i] = "P" }
			c == "q": { _list.[i] = "Q" }
			c == "r": { _list.[i] = "R" }
			c == "s": { _list.[i] = "S" }
			c == "t": { _list.[i] = "T" }
			c == "u": { _list.[i] = "U" }
